{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200,
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}